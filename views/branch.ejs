<div class="card">
    <ul class="list-group list-group-flush">
        <li class="list-group-item">
            Name:
            <%- branch.name%>
        </li>
        <li class="list-group-item">Address:
            <%-branch.address%>
        </li>
        <%if (isAuth){%>
            <li class="list-group-item">Active:
                <%-branch.active%>
            </li>
            <%}%>
                <li class="list-group-item">
                    <%- include('./branchMap', {address: branch.address, name:branch.name}); %>
                </li>

    </ul>
</div>